{
  "name": "nuxt-sample",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "opdykurihara <ykurihara@opendoor.co.jp>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path ../.gitignore .",
    "precommit": "npm run lint",
    "test": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/onesignal": "^3.0.0-beta.14",
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "@nuxtjs/vuetify": "^0.5.5",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "css-loader": "^2.1.1",
    "firebase": "^5.9.2",
    "nuxt": "^2.6.1",
    "preloadjs": "^1.0.1",
    "stats-js": "^1.0.1",
    "vuetify": "^1.5.14"
  },
  "devDependencies": {
    "@storybook/vue": "^5.0.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^24.1.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.3"
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
